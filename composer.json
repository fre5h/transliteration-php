{
    "name": "fresh/transliteration",
    "description": "PHP library for transliteration.",
    "keywords": ["transliteration", "transliterate", "Ukrainian", "English", "Russian"],
    "homepage": "https://github.com/fre5h/transliteration",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Artem Henvald",
            "email": "genvaldartem@gmail.com",
            "role": "Creator"
        }
    ],
    "support": {
        "email": "genvaldartem@gmail.com",
        "issues": "https://github.com/fre5h/transliteration/issues"
    },
    "require": {
        "php": ">=7.1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~6.4",
        "squizlabs/php_codesniffer": "~3.1"
    },
    "autoload": {
        "psr-4": {
            "Fresh\\Transliteration\\": "src/"
        },
        "exclude-from-classmap": [
            "/tests/"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\Fresh\\Transliteration\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.0.x-dev"
        }
    }
}
